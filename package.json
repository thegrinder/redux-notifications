{
  "name": "rr-notifications",
  "version": "1.1.4",
  "description": "Flexible react notification system",
  "keywords": [
    "react",
    "notifications"
  ],
  "homepage": "https://thegrinder.github.io/rr-notifications",
  "repository": {
    "type": "git",
    "url": "https://github.com/thegrinder/rr-notifications.git"
  },
  "main": "./dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "production": "webpack",
    "prepublishOnly": "npm run production",
    "test": "jest",
    "docz:build": "docz build",
    "docz:dev": "docz dev",
    "lint": "eslint './src/**/*.js' --fix",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "roots": [
      "<rootDir>/src"
    ]
  },
  "author": "Pawel Szczepkowski",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "docz": "^2.3.1",
    "docz-theme-default": "^1.2.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "semantic-release": "^17.1.1",
    "webpack": "4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "react": "^16.13.1"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  }
}
